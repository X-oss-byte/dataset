{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://tc39.es/dataset/schema/bundle.json",
  "title": "Bundle Proposals",
  "type": "array",
  "items": {
    "allOf": [
      {
        "type": "object",
        "properties": {
          "$schema": {
            "type": "string",
            "const": "https://tc39.es/dataset/schema/bundle.json"
          },
          "url": {
            "type": "string"
          },
          "pushed_at": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      {
        "$ref": "./individual.json"
      }
    ]
  }
}
